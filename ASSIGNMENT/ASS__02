import random
Humidity= random.randint(30,100)
Temperature=random.randint(30,100)
if(Temperature<60 and Humidity<55):
  print("NO HARM")
elif (Temperature<60 or Humidity<55):
  if (Temperature<60 or Humidity>55):
    print("HUMIDITY ALERT")
  elif(Temperature>60 or Humidity<55):
    print("TEMPERATURE ALERT")
else:
  print("THE AREA SHOULD BE CLEARED IMMEDIATELY")
